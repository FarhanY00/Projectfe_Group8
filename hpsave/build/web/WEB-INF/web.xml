<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.servlet.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>com.servlet.Register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddItem</servlet-name>
        <servlet-class>com.servlet.AddItem</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DelItemByID</servlet-name>
        <servlet-class>com.servlet.DelItemByID</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetItemData</servlet-name>
        <servlet-class>com.servlet.GetItemData</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetItemDataByID</servlet-name>
        <servlet-class>com.servlet.GetItemDataByID</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateItemByID</servlet-name>
        <servlet-class>com.servlet.UpdateItemByID</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddItem</servlet-name>
        <url-pattern>/AddItem</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DelItemByID</servlet-name>
        <url-pattern>/DelItemByID</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetItemData</servlet-name>
        <url-pattern>/GetItemData</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetItemDataByID</servlet-name>
        <url-pattern>/GetItemDataByID</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateItemByID</servlet-name>
        <url-pattern>/UpdateItemByID</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
